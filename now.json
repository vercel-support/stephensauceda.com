{
  "name": "stephensauceda.com",
  "alias": "stephensauceda.com",
  "version": 2,
  "builds": [
    { "src": "www/next.config.js", "use": "@now/next" },
    { "src": "rss/*.js", "use": "@now/node" }
  ],
  "build": {
    "env": {
      "API_ENDPOINT": "@api_endpoint",
      "ACCESS_TOKEN": "@access_token"
    }
  },
  "routes": [
    {
      "src": "/rss/(.*)",
      "dest": "/rss/$1"
    },
    {
      "src": "/",
      "continue": true,
      "headers": {
        "cache-control": "s-maxage=2592000, max-age=0"
      }
    },
    {
      "src": "/notes.*",
      "continue": true,
      "headers": {
        "cache-control": "s-maxage=3600, max-age=0"
      }
    },
    {
      "src": "/notes/(?<uid>[^/]+)",
      "dest": "www/notes/show?n=$uid"
    },
    { "src": "/(.*)", "dest": "/www/$1" }
  ]
}